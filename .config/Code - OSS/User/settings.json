{
    "vim.normalModeKeyBindings": [
    {
        "before": ["<Enter>"],
        "after": ["o", "<ESC>"]
    },
    {
        "before": ["S"],
        "after": ["i", "<Enter>", "<ESC>"]
    },
    {
        "before": ["g", "r"],
        "after": [":noh", "<Enter>"]
    },
    {
        "before": ["<Backspace>"],
        "after": ["i", "<Backspace>", "<ESC>", "l"]
    }
],
    "explorer.confirmDelete": false
}
