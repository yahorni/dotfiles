# https://khal.readthedocs.io/en/latest/index.html

[calendars]

[[calendars]]
path = ~/.local/share/calendars/*
type = discover
color = dark green

[locale]
timeformat = %H:%M
dateformat = %d/%m/%Y
longdateformat = %d/%m/%Y
datetimeformat = %d/%m/%Y %H:%M
longdatetimeformat = %d/%m/%Y %H:%M

[keybindings]
external_edit = E
