# vim: ft=dosini

[default]
highlight_event_days = true

[calendars]

[[calendars]]
path = ~/.local/share/calendars/*
type = discover

[locale]
timeformat = %H:%M
dateformat = %d/%m/%Y
longdateformat = %d/%m/%Y
datetimeformat = %d/%m/%Y %H:%M
longdatetimeformat = %d/%m/%Y %H:%M

[keybindings]
external_edit = E
